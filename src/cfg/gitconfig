[alias]
  ad = add -v
  br = branch -v
  ci = commit -v
  cl = clone -v
  co = checkout
  fe = fetch -v
  me = merge -v
  st = status
  add = add -v
  clone = clone -v
  commit = commit -v
  fetch = fetch -v
  merge = merge -v
  pull = pull -v
  push = push -v
  last = log -1 HEAD
  unstage = reset HEAD --

[remote "origin"]
        fetch = +refs/heads/*:refs/remotes/origin/*
